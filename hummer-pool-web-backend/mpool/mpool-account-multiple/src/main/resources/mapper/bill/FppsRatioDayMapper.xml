<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.mpool.accountmultiple.mapper.fpps.FppsRatioDayMapper">
	<select id="getRatio" resultType="com.mpool.common.model.account.fpps.FppsRatioDay">
		SELECT new_ratio,ratio FROM `fpps_ratio_day` WHERE `day`=#{day}  LIMIT 1
	</select>
</mapper>