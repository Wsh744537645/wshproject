<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.mpool.account.mapper.LanguageMapper">

	<select id="findByLangType"
		resultType="com.mpool.common.model.account.Language">
		SELECT
			*
		FROM
			account_language
		WHERE
			lang_type = #{langType}
	</select>
</mapper>
