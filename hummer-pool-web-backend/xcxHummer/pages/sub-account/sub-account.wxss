.tableHead{
  width: 100%;
  background: #1b48a8;
}
page{
  background-color: #fff;
}
.header{
  display: flex;
  flex-direction: row;
  border-radius:8px 8px 0px 0px;
  width: 90%;
  /* height: 74px; */
  margin: 15px auto 0 auto;
  background: #fff;
  color: #333;
}
.header .flex-item{
  border-bottom: none;
  text-align: center;
  font-size: 20px;
  border: 1px #e5e5e5 solid
}
.header .acc{
  width: 40%;
  border-radius:8px 0px 0px 0px;
  border-right: none;
  padding-top: 29px;
  font-size:19px;
  color: #000
}
.header .title{
  border-radius:0px 8px 0px 0px;
  width: 60%;
  font-size:19px;
  color: #000
}
.title text{
  display: inline-block;
  width: 50%
}

.summary{
  display: flex;
  flex-direction: row;
  width: 90%;
  height: 38px;
  background-color:rgba(250,250,250,1);
}
.flex-item3{
  height: 100%;
  display: flex;
  line-height: 100%;
  color: #646464;
  align-items:center;
  border: 1px #e5e5e5 solid;
  border-top: none;
  font-size: 20px;
}
.flex-item3 text{
  display: inline-block;
  width: 50%;
  text-align: center;  
}

.summary .time text{
  color: #646464;
  display: block;
  width: 50%;
  height: 35px;
  line-height: 35px;
  text-align: center;
  font-size: 15px;
}

.dataList{
  display: flex;
  flex-direction: row;
  width: 90%;
  height: 38px;
  background-color:rgba(250,250,250,1);
}
.nobackcolor{
  display: flex;
  flex-direction: row;
  width: 90%;
  height: 38px;
}
.flexItem{
  height: 100%;
  border: 1px #e5e5e5 solid;
  border-bottom: none;
  align-items:center;
  display: flex;
  font-size: 18px;
}
.dataList .user{
  display: inline-block;
  text-align: center;
  width: 40%;
  font-size: 18px;
  color: #13227a;
}
.dataList .user text{
  display: block;
  margin: 11px 0;
  color:rgba(27,72,168,1);
}
.nobackcolor .user{
  display: inline-block;
  text-align: center;
  width: 40%;
  font-size: 18px;
  color: #13227a;
}
.nobackcolor .user text{
  display: block;
  margin: 11px 0;
  color:rgba(27,72,168,1);
}
.flexItem image{
  width: 20px; 
  height: 20px;
  text-align: center;
  margin: 0 2px 0 5px;
}
.time{
  width: 60%;
}
.dataList .time text{
  color: #646464;
  display: block;
  width: 50%;
  height: 35px;
  line-height: 35px;
  text-align: center;
  font-size: 15px;
}
.nobackcolor .time text{
  color: #646464;
  display: block;
  width: 50%;
  height: 35px;
  line-height: 35px;
  text-align: center;
  font-size: 15px;
}
.pages{
  margin: 10rpx 0;
  width: 90%;
}
.size{
  color: #515253;
  float: left;
  text-align: left;
  font-size: 15px;
  font-family: "Source Sans Pro",sans-serif;
}
.total{
  height: 32px;
  line-height: 32px;
  color: #515253;
  float: left;
  text-align: left;
  font-size: 15px;
  font-family: "Source Sans Pro",sans-serif;
}
.size .num{
  margin: 10rpx;
  padding: 0 20rpx;
  border: 1px solid #e5e5e5
}
picker{
  display: inline-block
}
.current{
  width: 175rpx;
  /* background: #f8fafb; */
  color: #515253;
  float: right;
  text-align: right;
  font-size: 15px;
  font-family: "Source Sans Pro",sans-serif;
}
.current .num{
  margin: 10rpx;
  padding: 0 20rpx;
  border: 1px solid #e5e5e5
}